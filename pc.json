{
  "inbounds": [
    {
      "port": 1080,  // 监听端口为 1080，客户端通过此端口连接
      "listen": "::",  // 监听所有的IP地址（IPv6格式）
      "protocol": "socks",  // 使用 SOCKS5 协议
      "settings": {
        "auth": "noauth",  // 不需要认证
        "udp": true  // 允许 UDP 流量
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",  // 使用 vmess 协议
      "settings": {
        "vnext": [
          {
            "address": "tunnel-21-ipv6.dnsv1.cc",  // 服务器地址
            "port": 26464,  // 服务器端口
            "users": [
              {
                "id": "f4ebb7e7-a855-452f-d5aa-e66162105518",  // 用户ID
                "alterId": 0,  // 额外的ID
                "security": "auto",  // 加密方式
                "network": "tcp",  // 传输协议（tcp）
                "transport": {
                  "type": "none"  // 不使用伪装
                },
                "tls": false  // 不使用 TLS 加密
              }
            ]
          }
        ]
      },
      "tag": "tunnel-21-ipv6"  // 设置标签，方便路由选择
    }
  ]
}
