{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 1080,
      "listen": "::",
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "tunnel-21-ipv6.dnsv1.cc",  // 服务器1 地址
            "port": 15672,
            "users": [
              {
                "id": "f4ebb7e7-a855-452f-d5aa-e66162105518", 
                "alterId": 0,
                "security": "auto",
                "network": "tcp",
                "transport": {
                  "type": "none"
                },
                "tls": true,
                "host": ["http://yourdomain.com"],  // 伪装域名
                "path": "/yourpath"                 // 路径
              }
            ]
          }
        ]
      },
      "tag": "香港（路由器需要开启IPV6）"
    },
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "36.156.187.203",  // 服务器2 地址
            "port": 15672,
            "users": [
              {
                "id": "f4ebb7e7-a855-452f-d5aa-e66162105518", 
                "alterId": 0,
                "security": "auto",
                "network": "tcp",
                "transport": {
                  "type": "none"
                },
                "tls": true,
                "host": ["http://yourdomain.com"],  // 伪装域名
                "path": "/yourpath"                 // 路径
              }
            ]
          }
        ]
      },
      "tag": "香港-IPV4-默认使用这个"
    },
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "tunnel-2052-ipv6.666888.biz",  // 服务器3 地址
            "port": 15672,
            "users": [
              {
                "id": "f4ebb7e7-a855-452f-d5aa-e66162105518", 
                "alterId": 0,
                "security": "auto",
                "network": "tcp",
                "transport": {
                  "type": "none"
                },
                "tls": true,
                "host": ["http://newdomain.com"],  // 新伪装域名
                "path": "/newpath"                 // 新路径
              }
            ]
          }
        ]
      },
      "tag": "新服务器"
    },
    {
      "protocol": "freedom",
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "tag": "blocked"
    }
  ],
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "domain": ["geosite:cn"],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "ip": ["geoip:cn"],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "domain": ["geosite:geolocation-!cn"],
        "outboundTag": "香港（路由器需要开启IPV6）"
      },
      {
        "type": "field",
        "ip": ["0.0.0.0/0", "::/0"],
        "outboundTag": "香港（路由器需要开启IPV6）"
      },
      {
        "type": "field",
        "domain": ["geosite:geolocation-!cn"],
        "outboundTag": "香港-IPV4-默认使用这个"
      },
      {
        "type": "field",
        "ip": ["geoip:!cn"],
        "outboundTag": "新服务器"
      }
    ]
  },
  "dns": {
    "servers": [
      "8.8.8.8",
      "8.8.4.4",
      "2001:4860:4860::8888",
      "2001:4860:4860::8844"
    ]
  }
}
