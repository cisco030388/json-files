{
  "inbounds": [
    {
      "port": 1080,  // 监听端口为 1080，客户端通过此端口连接
      "listen": "::",  // 监听所有的IP地址（IPv6格式）
      "protocol": "socks",  // 使用 SOCKS5 协议
      "settings": {
        "auth": "noauth",  // 不需要认证
        "udp": true  // 允许 UDP 流量
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "socks",  // 使用 SOCKS5 协议
      "settings": {
        "servers": [
          {
            "address": "tunnel-21-ipv6.dnsv1.cc",  // 服务器地址
            "port": 31818,  // 服务器端口
            "users": [
              {
                "user": "xukun",  // 用户名
                "pass": "xukun@0303"  // 密码
              }
            ]
          }
        ]
      },
      "tag": "S5-2倍率-ipv6"  // 设置标签，方便路由选择
    }
  ]
}
