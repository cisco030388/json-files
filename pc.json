    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "36.156.187.203",  // 新服务器地址
            "port": 26464,                 // 端口
            "users": [
              {
                "id": "f4ebb7e7-a855-452f-d5aa-e66162105518",  // 用户ID (UUID)
