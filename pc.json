              {
                "id": "f4ebb7e7-a855-452f-d5aa-e66162105518",  // 用户ID (UUID)
                "alterId": 0,                                  // 额外ID
                "security": "auto",                             // 加密方式
                "network": "tcp",                               // 传输协议
                "transport": {
                  "type": "none"                                // 底层传输方式
                },
                "tls": false                                    // TLS关闭
              }
            ]
          }
        ]
      },
      "tag": "新服务器-IPV6"
    },
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "tunnel-21-ipv6.dnsv1.cc",  // 服务器地址
            "port": 26464,                          // 端口
